<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>ImageUpload</servlet-name>
        <servlet-class>edu.utexas.ee461l.slidesnap.servlets.ImageUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Uploaded</servlet-name>
        <servlet-class>edu.utexas.ee461l.slidesnap.servlets.Uploaded</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ImageUpload</servlet-name>
        <url-pattern>/imageupload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Uploaded</servlet-name>
        <url-pattern>/uploaded</url-pattern>
    </servlet-mapping>
</web-app>